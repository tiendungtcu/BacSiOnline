<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Dung Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Bác sỹ ơi | Tạo bệnh án</title>

    <!-- Fontfaces CSS-->
    <link href="~/Content/Common/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/Content/Common/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/Content/Common/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/select2/css/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Jcrop CSS-->
    <link href="~/Content/Common/jcrop/jquery.Jcrop.min.css" rel="stylesheet" media="all">
    <link href="~/Content/site.avatar.css" rel="stylesheet" media="all">

    <!-- Datatable -->
    <link href="~/Content/Common/datatable/DataTables-1.10.18/css/dataTables.bootstrap4.min.css" rel="stylesheet">   

    <!-- Main CSS-->
    <link href="~/Content/Common/css/theme.css" rel="stylesheet" media="all">
    <link href="~/Content/Common/css/style.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
    <div class="page-wrapper">
        @Html.Partial("_PartialLeftMenu")
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Tìm kiếm bệnh án..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">
                              
                                @Html.Partial("_PartialUserLogin")
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            @RenderBody()

            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="~/Content/Common/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Content/Common/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Content/Common/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Content/Common/slick/slick.min.js">
    </script>
    <script src="~/Content/Common/wow/wow.min.js"></script>
    <script src="~/Content/Common/animsition/animsition.min.js"></script>
    <script src="~/Content/Common/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Content/Common/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Content/Common/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Content/Common/circle-progress/circle-progress.min.js"></script>
    <script src="~/Content/Common/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Content/Common/select2/js/select2.min.js">
    </script>

    <!-- Main JS-->
    <script src="~/Content/Common/js/main.js"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>
<!-- end document-->
